// Switch case
#include <stdio.h>
#include<math.h>
int main() {
    int  OP, T;
    float RES;
    
    printf("\nintroduce la opci√≥n deseada: ");
    scanf("%d", &OP);
    printf("\nintroduce la variable a operar: ");   // Write C code here
    scanf("%d", &T);
        
    switch(OP){
        case 1:
            RES = (float) T/5;
            break;
        case 2:
            RES = pow(T,T);
             break;
        case 3:
        case 4:
            RES = 6*(float)T/2;
            break;    
        default:
            break;
    }
    printf("El resultado es: %f", RES);
    return 0;
}
